{
  "name": "react-video-trimmer",
  "version": "0.4.5",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "test": "cross-env NODE_ENV=test npm run eslint:src && jest --coverage && npm run typescript",
    "build": "npm run clean && npm run build:umd && npm run build:es",
    "build:umd": "cross-env NODE_ENV=es rollup -c",
    "build:es": "cross-env BABEL_ENV=es babel ./src --out-dir ./dist/es --ignore '**/*.spec.js'",
    "start": "styleguidist server",
    "styleguide": "styleguidist build",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "eslint:src": "eslint .",
    "commitmsg": "commitlint -e",
    "precommit": "pretty-quick --staged",
    "prepublish": "npm run build",
    "logo": "cd logo && sketchtool export artboards logo.sketch",
    "imagemin": "imagemin --out-dir=logo --plugin=pngquant --plugin=svgo",
    "size": "size-limit",
    "size:why": "size-limit --why",
    "typescript": "tsc --project ./typings/tests"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "7 KB"
    },
    {
      "path": "dist/es/index.js",
      "limit": "7 KB"
    }
  ],
  "keywords": [
    "react-component",
    "video",
    "trim",
    "ffmpeg",
    "react"
  ],
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "react": ">= 16.8"
  },
  "dependencies": {
    "attr-accept": "^1.1.3",
    "eslint-config-airbnb": "^17.1.1",
    "ffmpeg-webworker": "^0.4.3",
    "ffmpeg.js": "^3.1.9001",
    "file-selector": "^0.1.11",
    "hh-mm-ss": "^1.2.0",
    "prop-types": "^15.7.2",
    "react-player": "^1.11.1",
    "webpack-blocks": "^2.0.1",
    "webworker-file": "^0.2.0",
    "zero-fill": "^2.2.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-do-expressions": "^7.5.0",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-angular": "^7.0.1",
    "@commitlint/prompt": "^7.0.0",
    "@commitlint/prompt-cli": "^7.0.0",
    "@types/react": "^16.8.3",
    "@types/react-dom": "^16.8.1",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-polyfill": "^6.26.0",
    "commitizen": "^2.10.1",
    "cross-env": "^5.2.0",
    "husky": "^0.14.3",
    "imagemin-cli": "^3.0.0",
    "imagemin-pngquant": "^6.0.0",
    "lint-staged": "^7.2.2",
    "markdownlint-cli": "^0.13.0",
    "prettier": "*",
    "pretty-quick": "^1.11.1",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-styleguidist": "^9.0.1",
    "rimraf": "^2.5.2",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-copy": "^3.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-scss": "^1.0.1",
    "rollup-plugin-uglify": "^6.0.2",
    "sinon": "^3.2.1",
    "size-limit": "^0.19.2",
    "style-loader": "^0.18.2",
    "styled-components": "^4.1.2",
    "tslint": "^5.9.1",
    "typescript": "^3.2.4",
    "webpack": "^4.29.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/limistah/react-video-trimmer.git"
  },
  "typings": "typings/react-dropzone.d.ts",
  "config": {
    "commitizen": {
      "path": "@commitlint/prompt"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook"
    }
  },
  "engines": {
    "node": ">= 8"
  }
}
